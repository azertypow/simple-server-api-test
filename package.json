  "bugs": {
    "url": "https://github.com/azertypow/simple-server-api-test/issues"
  },
  "homepage": "https://github.com/azertypow/simple-server-api-test#readme",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "http-server": "^0.11.1"
  }
}
